<UserControl x:Class="Tischreservierung_Employee.TimeTabItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Tischreservierung_Employee"
             xmlns:wpfTool="clr-namespace:Xceed.Wpf.Toolkit;assembly=Xceed.Wpf.Toolkit"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="20"></RowDefinition>
            <RowDefinition Height="5*"></RowDefinition>
        </Grid.RowDefinitions>

        <wpfTool:DateTimePicker Name="TimePicker" Grid.Row="0" ValueChanged="TimePicker_ValueChanged">

        </wpfTool:DateTimePicker>

        <ListBox Grid.Row="1" Name="TimeListBox"></ListBox>
    </Grid>
</UserControl>
